<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<can_fest>
		<lib>libcanfestival_can_socket.so</lib>
		<interface>can0</interface>
		<baud>500K</baud>
	</can_fest>
	<mqtt>
		<broker>	
			<clientEndpoint>tcp://li554-27.members.linode.com:1883</clientEndpoint>
			<clientID>unused</clientID>
			<certificateFile>not used</certificateFile>
			<privateKeyFile>not used</privateKeyFile>
			<userId>B1_TEST_TOKEN</userId>
		</broker>
	</mqtt>
	<slave_nodes>
		<node id="a2"><name>device 2</name><nodeid>0x02</nodeid>
			<pdo type="tx" num="1">
				<cobid>0x182</cobid>
				<transmission_type>1</transmission_type><inhibit_time></inhibit_time><event_timer>0</event_timer>
			</pdo> 
			<pdo_map type="tx" num="1">
				<map><index>0x2000</index><subindex>1</subindex><num_bits>20</num_bits></map> 
			        <map><index>0x2000</index><subindex>2</subindex><num_bits>20</num_bits></map> 
			        <map><index>0x2000</index><subindex>3</subindex><num_bits>20</num_bits></map> 
			</pdo_map>
			                                                                                   
		</node>
		<node id="a3"><name>device 3</name><nodeid>0x03</nodeid>
			<pdo type="tx" num="1">
				<cobid>0x183</cobid>
				<transmission_type>1</transmission_type><inhibit_time></inhibit_time><event_timer>0</event_timer>
			</pdo> 
			<pdo_map type="tx" num="1">
				<map><index>0x2000</index><subindex>1</subindex><num_bits>20</num_bits></map> 
			        <map><index>0x2000</index><subindex>2</subindex><num_bits>20</num_bits></map> 
			        <map><index>0x2000</index><subindex>3</subindex><num_bits>20</num_bits></map> 
			</pdo_map>
		</node>

		<node id="a4"><name>device 4</name><nodeid>0x04</nodeid>
